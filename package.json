{
  "name": "@sentinel-hub/sentinelhub-js",
  "version": "1.0.0",
  "main": "dist/sentinelHub.cjs.js",
  "module": "dist/sentinelHub.esm.js",
  "browser": "dist/sentinelHub.umd.js",
  "dependencies": {
    "@turf/area": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@types/xml2js": "^0.4.4",
    "axios": "^0.18.1",
    "moment": "^2.24.0",
    "polygon-clipping": "^0.14.3",
    "query-string": "^6.4.2",
    "terraformer-wkt-parser": "^1.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@storybook/html": "^5.3.3",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "axios-mock-adapter": "^1.18.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.17.1",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^25.2.0",
    "typedoc": "^0.16.11",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "TZ=Europe/Ljubljana jest",
    "lint": "eslint --max-warnings 0 --ext .ts,js src/",
    "prettier": "prettier --write \"{src,example,stories}/**/*.{ts,js}\"",
    "storybook": "concurrently --kill-others \"rollup -c -w\" \"start-storybook -p 6006\"",
    "build-storybook": "build-storybook",
    "build-doc": "typedoc --excludePrivate --excludeProtected --excludeNotExported --exclude '**/*__*' --out doc/ src/"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "[.]fixtures[.]ts"
    ]
  },
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "dist/src/index.d.ts"
  ]
}
